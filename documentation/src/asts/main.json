{
  "ROUTES": {
    "HOME": {
      "component": "LazyAST",
      "props": {
        "source": "home"
      }
    },
    "INTRO": {
      "component": "LazyAST",
      "props": {
        "source": "introduction"
      }
    },
    "GETTING_STARTED": {
      "component": "LazyAST",
      "props": {
        "source": "getting-started"
      }
    },
    "RESOLVERS": {
      "component": "LazyAST",
      "props": {
        "source": "resolvers"
      }
    },
    "INTERPOLATION": {
      "component": "LazyAST",
      "props": {
        "source": "interpolation"
      }
    },
    "RENDERERS": {
      "component": "LazyAST",
      "props": {
        "source": "renderers"
      }
    },
    "CODE_SPLITTING": {
      "component": "LazyAST",
      "props": {
        "source": "code-splitting"
      }
    },
    "MATERIAL_STYLES": {
      "component": "LazyAST",
      "props": {
        "source": "material.styles"
      }
    },
    "CONDITIONAL": {
      "component": "LazyAST",
      "props": {
        "source": "conditional"
      }
    },
    "REACT_AST": {
      "component": "LazyAST",
      "props": {
        "source": "react-ast"
      }
    },
    "UTILITIES": {
      "component": "LazyAST",
      "props": {
        "source": "utilities"
      }
    }
  },
  "SWITCH_ROUTE": {
    "component": "Route",
    "module": "router",
    "props": {
      "exact": true
    }
  },
  "SUSPENSE": {
    "component": "Suspense",
    "module": "react",
    "props": {
      "fallback": {
        "$cmp": {
          "module": "react",
          "component": "Fragment"
        }
      }
    }
  },
  "MAIN": {
    "component": "Fragment",
    "module": "react",
    "children": [
      {
        "$cmp": "{{SUSPENSE}}",
        "children": [
          {
            "$cmp": {
              "component": "BrowserRouter",
              "module": "router",
              "props": {
                "basename": "{{$BASE_PATH}}"
              },
              "children": [
                {
                  "$cmp": "{{PAGE}}"
                }
              ]
            }
          }
        ]
      }
    ]
  },
  "BODY_ROUTES": {
    "component": "Switch",
    "module": "router",
    "children": [
      {
        "$cmp": "{{SWITCH_ROUTE}}",
        "props": {
          "path": "{{common.__vars.ROUTE_INTRODUCTION}}"
        },
        "children": [
          {
            "$cmp": "{{ROUTES.INTRO}}"
          }
        ]
      },
      {
        "$cmp": "{{SWITCH_ROUTE}}",
        "props": {
          "path": "{{common.__vars.ROUTE_HOME}}"
        },
        "children": [
          {
            "$cmp": "{{ROUTES.HOME}}"
          }
        ]
      },
      {
        "$cmp": "{{SWITCH_ROUTE}}",
        "props": {
          "path": "{{common.__vars.ROUTE_GETTING_STARTED}}"
        },
        "children": [
          {
            "$cmp": "{{ROUTES.GETTING_STARTED}}"
          }
        ]
      },
      {
        "$cmp": "{{SWITCH_ROUTE}}",
        "props": {
          "path": "{{common.__vars.ROUTE_INTERPOLATION}}"
        },
        "children": [
          {
            "$cmp": "{{ROUTES.INTERPOLATION}}"
          }
        ]
      },
      {
        "$cmp": "{{SWITCH_ROUTE}}",
        "props": {
          "path": "{{common.__vars.ROUTE_RESOLVERS}}"
        },
        "children": [
          {
            "$cmp": "{{ROUTES.RESOLVERS}}"
          }
        ]
      },
      {
        "$cmp": "{{SWITCH_ROUTE}}",
        "props": {
          "path": "{{common.__vars.ROUTE_RENDERERS}}"
        },
        "children": [
          {
            "$cmp": "{{ROUTES.RENDERERS}}"
          }
        ]
      },
      {
        "$cmp": "{{SWITCH_ROUTE}}",
        "props": {
          "path": "{{common.__vars.ROUTE_CODE_SPLITTING}}"
        },
        "children": [
          {
            "$cmp": "{{ROUTES.CODE_SPLITTING}}"
          }
        ]
      },
      {
        "$cmp": "{{SWITCH_ROUTE}}",
        "props": {
          "path": "{{common.__vars.ROUTE_MATERIAL_STYLES}}"
        },
        "children": [
          {
            "$cmp": "{{ROUTES.MATERIAL_STYLES}}"
          }
        ]
      },
      {
        "$cmp": "{{SWITCH_ROUTE}}",
        "props": {
          "path": "{{common.__vars.ROUTE_CONDITIONAL}}"
        },
        "children": [
          {
            "$cmp": "{{ROUTES.CONDITIONAL}}"
          }
        ]
      },
      {
        "$cmp": "{{SWITCH_ROUTE}}",
        "props": {
          "path": "{{common.__vars.ROUTE_REACTAST}}"
        },
        "children": [
          {
            "$cmp": "{{ROUTES.REACT_AST}}"
          }
        ]
      },
      {
        "$cmp": "{{SWITCH_ROUTE}}",
        "props": {
          "path": "{{common.__vars.ROUTE_UTILITIES}}"
        },
        "children": [
          {
            "$cmp": "{{ROUTES.UTILITIES}}"
          }
        ]
      },
      {
        "$cmp": {
          "module": "router",
          "component": "Redirect",
          "props": {
            "to": "{{common.__vars.ROUTE_HOME}}"
          }
        }
      }
    ]
  },
  "PAGE": {
    "component": "div",
    "styles": {
      "root": {
        "display": "flex",
        "minHeight": "100vh"
      }
    },
    "children": [
      {
        "$cmp": {
          "component": "CssBaseline",
          "module": "mui"
        }
      },
      {
        "$cmp": "{{APPBAR}}"
      },
      {
        "$cmp": "{{DRAWER}}"
      },
      {
        "$cmp": "{{BODY}}"
      }
    ]
  },
  "BODY": {
    "component": "div",
    "children": [
      {
        "$cmp": {
          "module": "mui",
          "component": "Toolbar"
        }
      },
      {
        "$cmp": "{{SUSPENSE}}",
        "children": [
          {
            "$cmp": {
              "component": "div",
              "children": [
                {
                  "$cmp": "{{BODY_ROUTES}}"
                }
              ],
              "props": {
                "style": {
                  "height": "100vh",
                  "padding": 20
                }
              }
            }
          }
        ]
      }
    ],
    "styles": {
      "root": {
        "flex": 1
      }
    }
  },
  "APPBAR": {
    "component": "AppBar",
    "module": "mui",
    "props": {
      "position": "fixed"
    },
    "styles": {
      "root": {
        "zIndex": 1000
      }
    },
    "children": [
      {
        "$cmp": "{{HEADER_TOOLBAR}}"
      }
    ]
  },
  "HEADER_TOOLBAR": {
    "component": "Toolbar",
    "module": "mui",
    "styles": {
      "root": {
        "display": "flex",
        "alignItems": "stretch",
        "justifyContent": "space-between"
      }
    },
    "children": [
      {
        "$cmp": "{{TITLE}}"
      },
      {
        "$cmp": "{{HEADER_LINKS}}"
      }
    ]
  },
  "TITLE": {
    "component": "div",
    "styles": {
      "root": {
        "display": "flex"
      }
    },
    "children": [
      {
        "$cmp": {
          "component": "Link",
          "module": "router",
          "props": {
            "to": "{{common.__vars.ROUTE_HOME}}"
          },
          "styles": {
            "root": {
              "color": "inherit",
              "textDecoration": "none",
              "display": "flex",
              "alignItems": "center"
            }
          }
        },
        "children": [
          {
            "$cmp": {
              "component": "div",
              "styles": {
                "root": {
                  "display": "flex"
                }
              },
              "children": [
                {
                  "$cmp": {
                    "component": "span",
                    "styles": {
                      "root": {
                        "display": "block",
                        "paddingRight": "1rem"
                      }
                    },
                    "children": [
                      "@zuze/react-ast"
                    ]
                  }
                },
                {
                  "$cmp": {
                    "module": "common",
                    "component": "Logo",
                    "styles": {
                      "root": {
                        "color": "white"
                      }
                    }
                  }
                }
              ]
            }
          }
        ]
      }
    ]
  },
  "HEADER_LINKS": {
    "component": "div",
    "styles": {
      "root": {
        "alignSelf": "center"
      }
    },
    "children": [
      {
        "$cmp": {
          "component": "IconButton",
          "module": "mui",
          "props": {
            "href": "{{$GITHUB}}",
            "target": "_blank"
          },
          "children": [
            {
              "$cmp": {
                "module": "icons",
                "component": "GitHub",
                "styles": {
                  "root": {
                    "color": "white"
                  }
                }
              }
            }
          ]
        }
      }
    ]
  },
  "DRAWER": {
    "component": "Drawer",
    "module": "mui",
    "props": {
      "variant": "permanent"
    },
    "children": [
      {
        "$cmp": {
          "module": "mui",
          "component": "Toolbar"
        }
      },
      {
        "$cmp": "{{NAV}}"
      }
    ],
    "styles": {
      "root": {
        "zIndex": 1,
        "width": "240px"
      },
      "paper": {
        "width": "240px"
      }
    }
  },
  "NAVLISTITEM": {
    "component": "ListItem",
    "module": "mui",
    "props": {
      "button": true
    },
    "styles": {
      "root": {
        "paddingLeft": "2rem"
      }
    }
  },
  "NAVLINK": {
    "component": "NavLink",
    "module": "router",
    "props": {
      "activeStyle": {
        "background": "{{common.__vars.ACTIVE_LINK_COLOR}}"
      }
    },
    "styles": {
      "root": {
        "display": "block",
        "textDecoration": "none",
        "color": "inherit"
      }
    }
  },
  "NAVLISTHEADER": {
    "component": "ListSubheader",
    "module": "mui"
  },
  "NAV": {
    "component": "List",
    "module": "mui",
    "children": [
      {
        "$cmp": "{{NAVLISTHEADER}}",
        "children": [
          "@zuze/react-ast"
        ]
      },
      {
        "$cmp": "{{NAVLINK}}",
        "props": {
          "to": "{{common.__vars.ROUTE_INTRODUCTION}}"
        },
        "children": [
          {
            "$cmp": "{{NAVLISTITEM}}",
            "children": [
              "Introduction"
            ]
          }
        ]
      },
      {
        "$cmp": "{{NAVLINK}}",
        "props": {
          "to": "{{common.__vars.ROUTE_GETTING_STARTED}}"
        },
        "children": [
          {
            "$cmp": "{{NAVLISTITEM}}",
            "children": [
              "Getting Started"
            ]
          }
        ]
      },
      {
        "$cmp": "{{NAVLINK}}",
        "props": {
          "to": "{{common.__vars.ROUTE_INTERPOLATION}}"
        },
        "children": [
          {
            "$cmp": "{{NAVLISTITEM}}",
            "children": [
              "Interpolation"
            ]
          }
        ]
      },
      {
        "$cmp": "{{NAVLINK}}",
        "props": {
          "to": "{{common.__vars.ROUTE_RESOLVERS}}"
        },
        "children": [
          {
            "$cmp": "{{NAVLISTITEM}}",
            "children": [
              "Resolvers"
            ]
          }
        ]
      },
      {
        "$cmp": "{{NAVLINK}}",
        "props": {
          "to": "{{common.__vars.ROUTE_RENDERERS}}"
        },
        "children": [
          {
            "$cmp": "{{NAVLISTITEM}}",
            "children": [
              "Renderers"
            ]
          }
        ]
      },
      {
        "$cmp": "{{NAVLISTHEADER}}",
        "children": [
          "Guides"
        ]
      },
      {
        "$cmp": "{{NAVLINK}}",
        "props": {
          "to": "{{common.__vars.ROUTE_CODE_SPLITTING}}"
        },
        "children": [
          {
            "$cmp": "{{NAVLISTITEM}}",
            "children": [
              "Code Splitting"
            ]
          }
        ]
      },
      {
        "$cmp": "{{NAVLINK}}",
        "props": {
          "to": "{{common.__vars.ROUTE_MATERIAL_STYLES}}"
        },
        "children": [
          {
            "$cmp": "{{NAVLISTITEM}}",
            "children": [
              "Material UI Styles"
            ]
          }
        ]
      },
      {
        "$cmp": "{{NAVLINK}}",
        "props": {
          "to": "{{common.__vars.ROUTE_CONDITIONAL}}"
        },
        "children": [
          {
            "$cmp": "{{NAVLISTITEM}}",
            "children": [
              "Conditional"
            ]
          }
        ]
      },
      {
        "$cmp": "{{NAVLISTHEADER}}",
        "children": [
          "API Reference"
        ]
      },
      {
        "$cmp": "{{NAVLINK}}",
        "props": {
          "to": "{{common.__vars.ROUTE_REACTAST}}"
        },
        "children": [
          {
            "$cmp": "{{NAVLISTITEM}}",
            "children": [
              "<ReactAST/>"
            ]
          }
        ]
      },
      {
        "$cmp": "{{NAVLINK}}",
        "props": {
          "to": "{{common.__vars.ROUTE_UTILITIES}}"
        },
        "children": [
          {
            "$cmp": "{{NAVLISTITEM}}",
            "children": [
              "Utilities"
            ]
          }
        ]
      }
    ]
  }
}